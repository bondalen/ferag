@prefix : <http://example.org/ferag/schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Organization a owl:Class .
:Company a owl:Class ;
    rdfs:subClassOf :Organization .
:Event a owl:Class .
:GEO a owl:Class .
:Person a owl:Class .
:City a owl:Class ;
    rdfs:subClassOf :GEO .
:Country a owl:Class ;
    rdfs:subClassOf :GEO .
:University a owl:Class ;
    rdfs:subClassOf :Organization .
:Project a owl:Class ;
    rdfs:subClassOf :Event .
:Program a owl:Class ;
    rdfs:subClassOf :Event .
:Workshop a owl:Class ;
    rdfs:subClassOf :Event .
:Consortium a owl:Class ;
    rdfs:subClassOf :Organization .

:employs a owl:ObjectProperty ;
    rdfs:domain :Company ;
    rdfs:range :Person .
:mentors a owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :Person .
:oversees a owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :Project .
:promotedTo a owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :Event .
:acquired a owl:ObjectProperty ;
    rdfs:domain :Company ;
    rdfs:range :Company .
:hasOfficeIn a owl:ObjectProperty ;
    rdfs:domain :Company ;
    rdfs:range :City .
:launched a owl:ObjectProperty ;
    rdfs:domain :Company ;
    rdfs:range :Program .
:involvedIn a owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :Project .
:concludedIn a owl:ObjectProperty ;
    rdfs:domain :Project ;
    rdfs:range :Event .
:mergedInto a owl:ObjectProperty ;
    rdfs:domain :Project ;
    rdfs:range :Program .
:worksOn a owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :Program .
:presentingAt a owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :Workshop .
:coPresentingAt a owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :Workshop .
:developed a owl:ObjectProperty ;
    rdfs:domain :Company ;
    rdfs:range :Project .
:partnerOf a owl:ObjectProperty ;
    rdfs:domain :Company ;
    rdfs:range :Consortium .
:foundingMemberOf a owl:ObjectProperty ;
    rdfs:domain :University ;
    rdfs:range :Consortium .
:researchPartnerOf a owl:ObjectProperty ;
    rdfs:domain :University ;
    rdfs:range :Consortium .
:supersededBy a owl:ObjectProperty ;
    rdfs:domain :Project ;
    rdfs:range :Consortium .
:usesForInference a owl:ObjectProperty ;
    rdfs:domain :Consortium ;
    rdfs:range :Program .
:usedInProductionStack a owl:ObjectProperty ;
    rdfs:domain :Consortium ;
    rdfs:range :Program .
:organizing a owl:ObjectProperty ;
    rdfs:domain :Consortium ;
    rdfs:range :Workshop .
:locatedIn a owl:ObjectProperty ;
    rdfs:domain :City ;
    rdfs:range :Country .