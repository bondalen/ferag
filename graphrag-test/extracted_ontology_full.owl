@prefix : <http://example.org/ferag/schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Organization a owl:Class .
:Company a owl:Class ;
    rdfs:subClassOf :Organization .
:Person a owl:Class .
:Event a owl:Class .
:GEO a owl:Class .
:City a owl:Class ;
    rdfs:subClassOf :GEO .
:Country a owl:Class ;
    rdfs:subClassOf :GEO .
:University a owl:Class ;
    rdfs:subClassOf :Organization .
:Project a owl:Class ;
    rdfs:subClassOf :Event .
:System a owl:Class ;
    rdfs:subClassOf :Project .
:Technology a owl:Class ;
    rdfs:subClassOf :Project .
:Team a owl:Class ;
    rdfs:subClassOf :Organization .

:worksAt a owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :Company .
:collaboratedWith a owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :Person .
:leads a owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :Team .
:joined a owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :Event .
:foundedIn a owl:ObjectProperty ;
    rdfs:domain :Company ;
    rdfs:range :City .
:operatesIn a owl:ObjectProperty ;
    rdfs:domain :Company ;
    rdfs:range :City .
:contributesTo a owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :Project .
:uses a owl:ObjectProperty ;
    rdfs:domain :System ;
    rdfs:range :Technology .
:initiatedIn a owl:ObjectProperty ;
    rdfs:domain :Project ;
    rdfs:range :Event .
:worksAtCompany a owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :Company .
:earnedPhDFrom a owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :University .
:isSpecialistIn a owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :Technology .
:isHeadOf a owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :Team .
:competesWith a owl:ObjectProperty ;
    rdfs:domain :Company ;
    rdfs:range :Company .
:operatesInMarket a owl:ObjectProperty ;
    rdfs:domain :Company ;
    rdfs:range :Technology .
:developedBy a owl:ObjectProperty ;
    rdfs:domain :System ;
    rdfs:range :Company .
:usesForEntityExtraction a owl:ObjectProperty ;
    rdfs:domain :System ;
    rdfs:range :Technology .
:usesForSchemaInduction a owl:ObjectProperty ;
    rdfs:domain :System ;
    rdfs:range :Technology .
:usesForVectorEmbeddings a owl:ObjectProperty ;
    rdfs:domain :System ;
    rdfs:range :Technology .
:usesForSemanticSearch a owl:ObjectProperty ;
    rdfs:domain :System ;
    rdfs:range :Technology .
:usesForGraphValidation a owl:ObjectProperty ;
    rdfs:domain :System ;
    rdfs:range :Technology .
:usesForGraphTraversal a owl:ObjectProperty ;
    rdfs:domain :System ;
    rdfs:range :Technology .
:usesForRelationalAnalytics a owl:ObjectProperty ;
    rdfs:domain :System ;
    rdfs:range :Technology .